opencti:
  url: 'http://localhost:8080'
  token: 'ChangeMe'

connector:
  id: 'ChangeMe'
  type: 'EXTERNAL_IMPORT'
  name: 'CXSecurity - Full'
  scope: 'identity,malware'
  confidence_level: 75 # From 0 (Unknown) to 100 (Fully trusted)
  update_existing_data: false
  log_level: 'info'
  label_color: '#33B5FF'

cxsecurity:
  interval: 20 # In hours, must be strictly greater than 1
